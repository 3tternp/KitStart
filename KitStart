logo="$(Banner)"

PromptFun(){
  echo ""
  echo "  ${orange}  â”Œâ”€â”€[${blue}Anlominus${purple}ðŸ‘½${red}KitStart${cyan}$~${orange}]${cyan}"
  echo "  ${orange}  â””â”€â”€â•¼${green}"
}
prompt="$(PromptFun)"

installAnlominusKits(){
  echo "${yellow} [!] ${GREEN} Installing Anlominus Kits"
  echo " "
  echo " "
  echo "${yellow} [!] ${orange} Copy: WorKing Repo"
  sleep 1
  git clone https://github.com/Anlominus/WorKing.git
  echo "${yellow} [!] ${orange} Copy: HacKing Repo"
  sleep 1
  git clone https://github.com/Anlominus/HacKing.git
  echo "${yellow} [!] ${orange} Copy: AnyGen Repo"
  sleep 1
  git clone https://github.com/Anlominus/AnyGen.git
  echo "${yellow} [!] ${orange} Copy: AnyPhishing Repo"
  sleep 1
  git clone https://github.com/Anlominus/AnyPhishing.git
  echo "${yellow} [!] ${orange} Copy: PenTest Repo"
  sleep 1
  git clone https://github.com/Anlominus/PenTest.git
  echo "${yellow} [!] ${orange} Copy: AnySpam Repo"
  sleep 1
  git clone https://github.com/Anlominus/AnySpam.git
  echo "${yellow} [!] ${orange} Copy: HacKits Repo"
  sleep 1
  git clone https://github.com/Anlominus/HacKits.git
  echo "${yellow} [!] ${orange} Copy: FrameWorks Repo"
  sleep 1
  git clone https://github.com/Anlominus/FrameWorks.git
  echo "${yellow} [!] ${orange} Copy: CheatSheets Repo"
  sleep 1
  git clone https://github.com/Anlominus/CheatSheets.git
  echo "${yellow} [!] ${orange} Copy: AntiPorn Repo"
  sleep 1
  git clone https://github.com/Anlominus/AntiPorn.git
  sudo chmod -R +x ./*
}

# this is the main function
menu() {
  while true; do
    clear
    echo "$logo"
    echo " "
    echo "${green}     [g] ${GREEN}Get ${orange}Anlominus ${ORANGE}Kits"
    echo " "
    echo "${green}     [1] ${GREEN}WorKing"
    echo "${green}     [2] ${REDBG}${orange}Hac${WHITE}King${nc}"
    echo "${green}     [3] ${orange}Any${ORANGE}Gen"
    echo "${green}     [4] ${orange}Any${ORANGE}Phishing"
    echo "${green}     [5] ${MAGENTA}Pen${blue}Test"
    echo "${green}     [6] ${orange}Any${RED}Spam"
    echo "${green}     [7] ${orange}Frame${GREEN}Works"
    echo "${green}     [8] ${orange}Hac${ORANGE}Kits"
    echo "${green}     [9] ${orange}Cheat${orange}Sheets"
    echo "${green}     [10] ${white}Anti${cyan}porn"
    echo " "
    echo "${green}     [x] ${green} Exit"
    echo "$prompt"
    read cmd
    case ${cmd} in
      g)
      installAnlominusKits
      ;;
      1)
      cd WorKing
      sh WorKing start
      cd ..
      ;;
      2)
      cd HacKing
      sh HacKing start
      cd ..
      ;;
      3)
      cd AnyGen
      sh AnyGen start
      cd ..
      ;;
      4)
      cd AnyPhishing
      sh AnyPhishing start
      cd ..
      ;;
      5)
      cd PenTest
      sh PenTest start
      cd ..
      ;;
      6)
      cd AnySpam
      sh AnySpam start
      cd ..
      ;;
      7)
      cd FrameWorks
      sh FrameWorks start
      cd ..
      ;;
      8)
      cd HacKits
      sh HacKits start
      cd ..
      ;;
      9)
      cd CheatSheets
      sh CheatSheets start
      cd ..
      ;;
      10)
      cd AntiPorn
      sh AntiPorn start
      cd ..
      ;;
      x|exit|Exit)
      echo "${green}Exiting ....${nc}"
      break
      ;;
      *)
        echo "${red}${cmd}: ${purple}invalid option!!${nc}"
        sleep 1
      ;;
    esac
  done
}

# cli mode
if [ $# -eq 1 ]; then
  case $1 in
    start)
      menu
    ;;
    help|-h)
      echo "${yellow}KitStart ${red} v0.1"
      echo "${nc}Usage ${nc}: ${yellow} KitStart ${nc}[option]... [toolname]...${nc}"
      echo "\n  ${blue}Option  \tDescription${nc}"
      echo "  -----------  \t--------------------"
      echo "  start  \tstart KitStart menu mode."
    ;;
    *)
      echo "${red}Sorry ${nc}: '$1' ${red} invalid option!!${nc}"
      echo "${blue}type ${nc}:\n  'KitStart -h' or 'KitStart help' for help.${nc}"
    ;;
  esac
elif [ $# -eq 2 ]; then
  case $1 in
    *)
      echo "${red}Sorry ${nc}: '$1' ${red} invalid option!!${nc}"
      echo "${blue}type ${nc}:\n  'KitStart -h' or 'KitStart help' for help.${nc}"
    ;;
  esac
else
  echo "${blue}type ${nc}:\n  'KitStart -h' or 'KitStart help' for help.${nc}"
fi
